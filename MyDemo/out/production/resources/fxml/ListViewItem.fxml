<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXRippler?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.*?>
<HBox fx:id="root" prefHeight="80.0" prefWidth="Infinity" xmlns="http://javafx.com/javafx/8.0.172-ea"
      xmlns:fx="http://javafx.com/fxml/1" fx:controller="wu.controllers.ListViewItemController">

    <padding>
        <Insets top="18" bottom="10" />
    </padding>

    <JFXRippler maskType="RECT"
                style="-jfx-rippler-fill:  #808080;">

        <HBox style="-fx-background-color: #fff">

            <ImageView fx:id="imageView" fitHeight="80" fitWidth="80" />

            <Label
                maxWidth="300"
                maxHeight="Infinity"
                style="-fx-font-size: 17; -fx-wrap-text: true; -fx-text-fill: #666666"
                text="人教版新编同步AB卷全新ab卷三年级下册人教版新编同步AB卷全新ab卷三年级下册人教版新编同步AB卷全新ab卷三年级下册人教版新编同步AB卷全新ab卷三年级下册">

                <HBox.margin>
                    <Insets left="25" top="15" bottom="15"/>
                </HBox.margin>

            </Label>

            <VBox spacing="5" alignment="CENTER">

                <HBox.margin>
                    <Insets left="40"/>
                </HBox.margin>

                <padding>
                    <Insets bottom="15.0" left="10.0" right="10.0" top="15.0" />
                </padding>

                <Label
                    style="-fx-text-fill: #333333; -fx-font-weight: bold; -fx-font-size: 20"
                    text="19.8"/>
                <Label
                    style="-fx-text-fill: #999999; -fx-font-size: 15"
                    text="原价"/>
            </VBox>

            <VBox spacing="5" alignment="CENTER">

                <padding>
                    <Insets bottom="15.0" left="10.0" right="10.0" top="15.0" />
                </padding>

                <Label
                    style="-fx-text-fill: #333333; -fx-font-weight: bold; -fx-font-size: 20"
                    text="19.8"/>
                <Label
                    style="-fx-text-fill: #999999; -fx-font-size: 15"
                    text="售价"/>
            </VBox>

            <VBox spacing="5" alignment="CENTER">

                <padding>
                    <Insets bottom="15.0" left="10.0" right="10.0" top="15.0" />
                </padding>

                <Label
                    style="-fx-text-fill: #333333; -fx-font-weight: bold; -fx-font-size: 20"
                    text="19.8"/>
                <Label
                    style="-fx-text-fill: #999999; -fx-font-size: 15"
                    text="盈利"/>
            </VBox>

            <StackPane alignment="BOTTOM_CENTER">

                <padding>
                    <Insets bottom="15.0" left="40.0" right="10.0" top="10.0" />
                </padding>


                <!--<JFXButton fx:id="resendBt"/>-->

                <JFXRippler
                    position="BACK"
                    styleClass="icons-rippler"
                    style="-fx-max-width: 30; -fx-max-height: 30; -jfx-rippler-fill: #808080;">

                    <StackPane.alignment>TOP_RIGHT</StackPane.alignment>
                    <Label fx:id="refreshLabel"
                           StackPane.alignment="CENTER"
                           alignment="CENTER"
                           visible="false"
                           style="-fx-max-width: 30; -fx-max-height: 30;"/>

                </JFXRippler>

                <JFXRippler
                    position="BACK"
                    styleClass="icons-rippler"
                    style="-fx-max-width: 30; -fx-max-height: 30; -jfx-rippler-fill: #808080;">

                    <StackPane.margin>
                        <Insets right="30"/>
                    </StackPane.margin>

                    <StackPane.alignment>TOP_RIGHT</StackPane.alignment>
                    <Label fx:id="editLabel"
                           StackPane.alignment="CENTER"
                           alignment="CENTER"
                           visible="false"
                           style="-fx-max-width: 30; -fx-max-height: 30;"/>

                </JFXRippler>

                <JFXRippler
                    position="BACK"
                    styleClass="icons-rippler"
                    style="-fx-max-width: 30; -fx-max-height: 30; -jfx-rippler-fill: #808080;">

                    <StackPane.margin>
                        <Insets right="60"/>
                    </StackPane.margin>

                    <StackPane.alignment>TOP_RIGHT</StackPane.alignment>
                    <Label fx:id="deleteLabel"
                           StackPane.alignment="CENTER"
                           alignment="CENTER"
                           visible="false"
                           style="-fx-max-width: 30; -fx-max-height: 30;"/>

                </JFXRippler>

                <Label
                    fx:id="desLabel"
                    style="-fx-text-fill: #fc4426; -fx-font-size: 14"
                    text="发布失败, 请重试">
                </Label>
            </StackPane>
        </HBox>

    </JFXRippler>


</HBox>

